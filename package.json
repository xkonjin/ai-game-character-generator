  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/aiplatform": "^3.0.0",
    "axios": "^1.6.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "form-data": "^4.0.0",
    "openai": "^4.0.0",
    "ora": "^8.0.0",
    "sharp": "^0.33.0",
    "three": "^0.160.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/three": "^0.160.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
