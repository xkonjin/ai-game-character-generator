{
  "name": "ai-game-character-generator",
  "version": "1.0.0",
  "description": "End-to-end AI pipeline for creating animated 3D game characters",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "generate": "ts-node src/cli/index.ts generate",
    "image-gen": "ts-node src/cli/index.ts image-gen",
    "animate": "ts-node src/cli/index.ts animate",
    "rig-3d": "ts-node src/cli/index.ts rig-3d",
    "export": "ts-node src/cli/index.ts export",
    "test": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "game-development",
    "character-generator",
    "pixel-art",
    "threejs",
    "3d-rigging",
    "veo",
    "tripo"
