{
  "name": "ai-game-character-generator",
  "version": "1.0.0",
  "description": "End-to-end AI pipeline for creating animated 3D game characters",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/index.ts",
    "generate": "tsx src/cli/index.ts generate",
    "image-gen": "tsx src/cli/index.ts image-gen",
    "animate": "tsx src/cli/index.ts animate",
    "rig-3d": "tsx src/cli/index.ts rig-3d",
    "export": "tsx src/cli/index.ts export",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "game-development",
    "character-generator",
    "pixel-art",
    "threejs",
    "3d-rigging",
    "veo",
    "tripo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/aiplatform": "^6.0.0",
    "axios": "^1.6.0",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.0",
    "openai": "^6.16.0",
    "ora": "^9.0.0",
    "sharp": "^0.34.5",
    "three": "^0.182.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
